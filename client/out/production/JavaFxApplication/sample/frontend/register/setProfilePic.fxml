<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="528.0" prefWidth="651.0" style="-fx-background-color: white; -fx-border-style: solid;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.frontend.register.ProfilePicController">
   <children>
      <ImageView fx:id="imageView" fitHeight="172.0" fitWidth="196.0" layoutX="236.0" layoutY="73.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../feed/photos/userProf1.png" />
         </image>
      </ImageView>
      <Button fx:id="openButton" layoutX="249.0" layoutY="293.0" mnemonicParsing="false" onAction="#onFileChooserClick" prefHeight="35.0" prefWidth="147.0" text="Choose a photo" textOverrun="CENTER_WORD_ELLIPSIS" />
      <Button fx:id="cancelButton" layoutX="122.0" layoutY="353.0" mnemonicParsing="false" onAction="#onCancelClick" prefHeight="58.0" prefWidth="79.0" text="cancel" />
      <Button fx:id="saveButton" layoutX="439.0" layoutY="353.0" mnemonicParsing="false" onAction="#onSaveClick" prefHeight="58.0" prefWidth="106.0" text="save" />
      <Label fx:id="alert" alignment="CENTER" layoutX="185.0" layoutY="428.0" prefHeight="52.0" prefWidth="274.0" textFill="#fc0404">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
